<?xml version="1.0" encoding="UTF-8"?>
<!--映射文件mapper-->
<!DOCTYPE mapper PUBLIC
        "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.gok.dao.sys.ISysMemberDao">
    <resultMap id="resultMapGoods" type="cn.gok.beans.sys.sysMember">
        <id column="CUSTOMER_INF_ID" property="customerInfId"/>
        <result column="CUSTOMER_ID" property="customerId"/>
        <result column="CUSTOMER_NAME" property="customerName"/>
        <result column="REAL_NAME" property="realName"/>
        <result column="GENDER" property="gender"/>
        <result column="IDENTY_CARD_TYPE" property="identyCardType"/>
        <result column="IDENTY_CARD_NO" property="identyCardNo"/>
        <result column="BIRTHDAY" property="birthday"/>
        <result column="USER_MOBILE" property="userMobile"/>
        <result column="EMAIL" property="email"/>
        <result column="USER_POINT" property="userPoint"/>
        <result column="REGISTER_TIME" property="registerTime"/>
        <result column="CUSTOMER_STATUS" property="customerStatus"/>
        <result column="IS_DEL" property="idDel"/>
        <result column="CUSTOMER_LEVEL" property="customerLevel"/>
        <result column="USER_BALACE" property="userBalance"/>
        <result column="REMARK" property="remark"/>
        <result column="MODIFIED_TIME" property="modifiedTime"/>

    </resultMap>

    <insert id="saveMember" parameterType="cn.gok.beans.sys.sysMember">
        insert into c_customer_info (CUSTOMER_INF_ID,CUSTOMER_ID,CUSTOMER_NAME,REAL_NAME,IDENTY_CARD_TYPE,IDENTY_CARD_NO,USER_MOBILE,EMAIL,GENDER,BIRTHDAY,CITY) values (#{customerInfId},#{customerId},#{customerName},#{realName},#{identyCardType},#{identyCardNo},#{userMobile},#{email},#{gender},#{birthday},#{city})
    </insert>



</mapper>